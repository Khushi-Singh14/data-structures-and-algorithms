class Solution {
public:
    void duplicateZeros(vector<int>& arr) {
        int i = 0;
        int n = arr.size();
        int j = n - 1;
        while (i<n && j<n) {
            if (arr[i] == 0) {
                j=n-1;
                while (i < j) {
                    arr[j] = arr[j - 1];
                    j--;
                }
                i+=2;
            }
            else i++;
        }
    }
};
