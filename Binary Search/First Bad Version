class Solution {
public:
    int firstBadVersion(int n) {
        int low=1;
        int high=n;
        int ans;
        while(low<=high){
            int mid=low + (high-low)/2;
            bool res=isBadVersion(mid);
            if(res){
                ans=mid;
                high=mid-1;
            }
            else{
                low=mid+1;
            }
        }
        return ans;        
    }
};
