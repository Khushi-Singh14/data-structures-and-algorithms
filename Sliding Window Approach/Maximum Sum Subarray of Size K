class Solution {
  public:
    int maxSubarraySum(vector<int>& arr, int k) {
        // code here
        int low=0;
        int high=k-1;
        int sum=0;
        int n=arr.size();
        for(int i=low;i<=high;i++){
            sum=sum+arr[i];
        }
        int res=sum;
        while(high<n){
            res=max(sum,res);
            high++; low++;
            sum=sum-arr[low-1]+arr[high];
        }
        return res;
    }
};
