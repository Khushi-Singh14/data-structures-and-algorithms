class Solution {
public:
    int maxAbsoluteSum(vector<int>& nums) {
        int minEnd=nums[0];
        int maxEnd=nums[0];
        int res=abs(nums[0]);
        for(int i=1;i<nums.size();i++){
            minEnd=min(minEnd+nums[i],nums[i]);
            maxEnd=max(maxEnd+nums[i],nums[i]);
            res=max(res,max(maxEnd,abs(minEnd)));
        }
        return res;
    }
};
