class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int>f;
        int n=nums.size();
        int req=n/2;
        int res=0;
        for(int i=0;i<n;i++){
            f[nums[i]]++;
        }
        for(auto i:f){
            if(i.second>req) res=i.first;
        }
        return res;
    }
};
