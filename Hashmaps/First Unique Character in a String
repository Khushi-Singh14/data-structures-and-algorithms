class Solution {
public:
    int firstUniqChar(string s) {
        unordered_map<char,int>f;
        int res=-1;
        for(int i=0;i<s.size();i++){
            f[s[i]]++;
        }
        for(int i=0;i<s.size();i++){
            if(f[s[i]]==1){
                res=i;
                break;
            } 
        }
        return res;       
    }
};
